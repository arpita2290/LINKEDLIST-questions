#include <stdio.h>
#define MAX 3

int stack[MAX];
int top = -1;

void push(int x) {
    if (top == MAX - 1) {
        printf("Stack Overflow\n");
        return;
    }
    stack[++top] = x;
}

int pop() {
    if (top == -1) {
        printf("Stack Underflow\n");
        return -1;
    }
    return stack[top--];
}

void display() {
    for (int i = top; i >= 0; i--)
        printf("%d ", stack[i]);
    printf("\n");
}

int main() {
    push(10); display();
    push(20); display();
    push(30); display();
    push(40); // Overflow
    pop(); display();
    pop(); display();
    pop(); display();
    pop(); // Underflow
    return 0;
}
